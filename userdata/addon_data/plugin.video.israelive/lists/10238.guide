[{"tvGuide": [{"start": 1488680100, "image": null, "end": 1488685800, "name": "One Direction: This Is Us", "description": "This inspiring feature film tells the incredible story of Niall, Zayn, Liam, Louis and Harry’s catapulting rise to fame from humble beginnings on the X-Factor to conquering the world! A captivating and intimate all-access look at life on the road for the worldwide music sensation"}, {"start": 1488685800, "image": null, "end": 1488687300, "name": "The Insider", "description": "The Insider reports on the world of celebrities from a unique  insider  perspective, using first-person interviews, special  inside  access and  behind-closed-door  reports"}, {"start": 1488687300, "image": null, "end": 1488693480, "name": "Justin Bieber: Never Say Never", "description": "The inspiring true story and rare inside look at the rise of Justin Bieber from a street performer in the small town of Stratford, Ontario, to an internet phenomenon to global super star, culminating with a dream sold out show, soon after his 15th birthday, at the famed Madison Square Garden"}, {"start": 1488693480, "image": null, "end": 1488698640, "name": "Cloudy With A Chance Of Meatballs", "description": "When hard times hit the town of Swallow Falls, a failed inventor constructs a device that turns water into delicious food. Animation- Bill Hader / Anna Faris"}, {"start": 1488698640, "image": null, "end": 1488704700, "name": "Tangled", "description": "Get tangled up in fun and adventure in this musical. When Flynn, a charming bandit, hides in a mysterious tower, the last thing he expects to find is Rapunzel, a spirited teen with 70 feet of magical golden hair! As they set out on a fantastic trip, let your hair down and get ready to cheer"}, {"start": 1488704700, "image": null, "end": 1488711300, "name": "Wreck-It Ralph", "description": "Tired of being overshadowed by Fix-It Felix, the ‘good guy’; Ralph, a video game villain, takes matters into his own hands to fulfill his dream of becoming a hero and sets off on a game-hopping journey across the arcade through every generation of video games to prove he s got what it takes!"}, {"start": 1488711300, "image": null, "end": 1488718800, "name": "Being John Malkovich", "description": "A puppeteer discovers a door in his office that allows him to enter the mind of John Malkovich. The puppeteer then tries to turn the portal into a small business"}, {"start": 1488718800, "image": null, "end": 1488724200, "name": "A Dangerous Method", "description": "Seduced by a challenging case, Dr. Jung takes unbalanced yet beautiful Sabina as his patient, using the methods of his master, Sigmund Freud. Deep impulses, dark desires and untold secrets change everything as both men fall under Sabina s spell"}, {"start": 1488724200, "image": null, "end": 1488726000, "name": "The Insider", "description": "The Insider reports on the world of celebrities from a unique  insider  perspective, using first-person interviews, special  inside  access and  behind-closed-door  reports"}, {"start": 1488726000, "image": null, "end": 1488733200, "name": "Atonement", "description": "Fledgling writer Briony Tallis, as a 13-year-old, irrevocably changes the course of several lives when she accuses her older sister s lover of a crime he did not commit.Drama"}, {"start": 1488733200, "image": null, "end": 1488740400, "name": "The Constant Gardener", "description": "A widower is determined to get to the bottom of a potentially explosive secret involving his wife s murder, big business, and corporate corruption"}, {"start": 1488740400, "image": null, "end": 1488747600, "name": "Labor Day", "description": "An escaped convict manipulates an emotionally fragile single mother into letting him hide out at her house even as the police search the town for him"}, {"start": 1488747600, "image": null, "end": 1488753300, "name": "Tangled", "description": "Get tangled up in fun and adventure in this musical. When Flynn, a charming bandit, hides in a myster